{% extends "/templates/base.jinja2" %}

{% block content %}
    <p>
        This page displays the title of each of your surveys by using the SurveyMonkey API.
    </p>
    <table>
        <thead>
            <th>Title</th>
        </thead>
        <tbody>
            {% for survey in surveys %}
            <tr>
                <td><a href="{{ survey.collect_url }}" target="_blank">{{ survey.title }}</a></td>
            <tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
